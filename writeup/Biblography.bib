	Journal = {Current Directions in Psychological Science},
	Number = {1},
	Pages = {19-23},
	Title = {{Cognitive and Linguistic Processing in the Bilingual Mind}},
	Url = {http://cdp.sagepub.com/content/19/1/19.abstract},
	Volume = {19},
	Year = {2010},
	Bdsk-Url-1 = {http://cdp.sagepub.com/content/19/1/19.abstract},
