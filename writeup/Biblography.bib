	Number = {1},
	Pages = {1--44},
	Publisher = {AI Access Foundation},
	Title = {Acquiring word-meaning mappings for natural language interfaces},
	Volume = {18},
	Year = {2003},
	Bdsk-Url-1 = {http://www.aaai.org/Papers/JAIR/Vol18/JAIR-1801.pdf}}

@article{Vogt_2002,
	Abstract = {In this paper an experiment is presented in which two mobile robots develop a shared lexicon of which the meanings are grounded in the real world. The robots start without a lexicon nor shared meanings and play language games in which they generate new meanings and negotiate words for these meanings. The experiment tries to find the minimal conditions under which verbal communication may begin to evolve. The robots are autonomous in terms of computing and cognition, but they are otherwise far simpler than most, if not all animals. It is demonstrated that a lexicon nevertheless can be made to emerge even though there are strong limits on the size and stability of this lexicon},
	Author = {Vogt, Paul},
