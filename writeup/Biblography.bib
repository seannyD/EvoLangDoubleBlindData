	Journal = {Bilingualism: Language and Cognition},
	Keywords = {Colour; Categorical Perception; Bilingualism},
	Number = {01},
	Pages = {83-95},
	Title = {Cognitive representation of colour in bilinguals: The case of Greek blues},
