@mastersthesis{Line_2010,
	Abstract = {The Human Iterated Learning Model pioneered by Kirby, Cornish and Smith (2008) offers an alternative to computational and mathematical models for the investigation of language evolution. However up to now, research using this model has been limited to vertical cultural transmission. This study presents a new framework for investigating horizontal transmission inspired by earlier Iterated Learning experiments. It was shown that the languages displayed increased learnability and structure over time, confirming the findings of previous studies of vertical transmission. Furthermore, in contrast to these studies, compositionality was shown to emerge without the need for an artificial filter for ambiguity, indicating that horizontal transmission warrants further investigation.},
	Author = {Phillipa Line},
	Date-Added = {2012-01-06 11:56:48 +0000},
